{
  "expo": {
    "name": "APLS",
    "slug": "APLS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/firefightericon.png",
    "splash": {
      "image": "./assets/firefightericon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/sounds/am_coming.wav"]
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ermiceo.APLS"
    },
    "web": {
      "favicon": "./assets/firefightericon.png"
    },
    "notification":{
      "icon":"./assets/firefightericon.png",
      "color":"#ff4e47"
    }
  }
}
